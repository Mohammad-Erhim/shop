    "nodemon": "^3.1.7",
    "prettier": "^3.4.2",
    "quicktype": "^23.0.170",
    "sequelize-cli": "^6.6.2",
    "supertest": "^7.0.0",
    "ts-jest": "^29.2.5",
    "ts-node": "^10.9.2",
    "ts-node-dev": "^2.0.0",
