  "bugs": {
    "url": "https://github.com/Mohammad-Erhim/shop/issues"
  },
  "homepage": "https://github.com/Mohammad-Erhim/shop#readme",
  "description": "",
  "dependencies": {
    "bcrypt": "^5.1.1",
    "bcryptjs": "^2.4.3",
    "dotenv": "^16.4.7",
    "express": "^4.21.2",
    "express-rate-limit": "^7.5.0",
    "google-auth-library": "^9.15.0",
    "googleapis": "^144.0.0",
    "i18next": "^24.1.0",
    "jsonwebtoken": "^9.0.2",
    "mime-types": "^2.1.35",
    "minio": "^8.0.2",
    "multer": "^1.4.5-lts.1",
    "node-cron": "^3.0.3",
    "pg": "^8.13.1",
    "pg-hstore": "^2.3.4",
    "sequelize": "^6.37.5",
    "socket.io": "^4.8.1",
    "socket.io-client": "^4.8.1",
    "winston": "^3.17.0",
    "zod": "^3.24.1",
    "zod-i18n-map": "^2.27.0"
  },
  "devDependencies": {
    "@babel/register": "^7.25.9",
    "@eslint/js": "^9.16.0",
    "@socket.io/admin-ui": "^0.5.1",
    "@types/bcrypt": "^5.0.2",
    "@types/express": "^5.0.0",
    "@types/jest": "^29.5.14",
    "@types/jsonwebtoken": "^9.0.7",
    "@types/mime-types": "^2.1.4",
    "@types/multer": "^1.4.12",
