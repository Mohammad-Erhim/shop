{
  "name": "server-boilerplate",
  "version": "1.0.0",
  "main": "index.js",
  "scripts": {
    "dev": "ts-node-dev --respawn --transpile-only src/server.ts",
    "build": "tsc && npm run copy-static-files",
    "copy-static-files": "copyfiles -u 1 src/locales/**/* dist",
    "start": "node dist/server.js",
    "test:e2e": "jest --config jest.config.ts",
    "seed:all": "npx sequelize-cli db:seed:all",
    "format": "prettier --write .",
    "local-types": "npx quicktype -o src/types/languageTranslations.ts -l ts src/locales/en.json && prettier --write ."
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Mohammad-Erhim/shop.git"
  },
